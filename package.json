{
  "name": "webflow-code-components",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "styles": "node lib/styles/scripts/transform-css-vars.js",
    "theme:refresh": "node lib/styles/scripts/transform-css-vars.js default.css variables.webflow.css",
    "webflow:share": "NEXT_PUBLIC_API_URL=https://blogflow-three.vercel.app NEXT_PUBLIC_GOOGLE_OAUTH_ENABLED=false npx webflow library share --no-input",
    "webflow:bundle": "NEXT_PUBLIC_API_URL=https://blogflow-three.vercel.app NEXT_PUBLIC_GOOGLE_OAUTH_ENABLED=false npx webflow library bundle --public-path http://localhost:4000/ --dev && npx serve dist -p 4000",
    "webflow:bundle:ci": "npx webflow library bundle",
    "webflow:bundle:debug": "NEXT_PUBLIC_API_URL=https://blogflow-three.vercel.app NEXT_PUBLIC_GOOGLE_OAUTH_ENABLED=false npx webflow library bundle --public-path http://localhost:4000/ --dev --debug-bundler && npx serve dist -p 4000",
    "webflow:bundle:size": "du -sh dist/ && echo '\nDetailed sizes:' && du -h dist/Client/ | sort -h",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "NODE_TLS_REJECT_UNAUTHORIZED=0 tsx --env-file=.env lib/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "NODE_TLS_REJECT_UNAUTHORIZED=0 drizzle-kit studio"
  },
